{
    "compilerOptions": {
        "target": "ES5",
        "module": "CommonJS",
        "outDir": "./dist",
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
}